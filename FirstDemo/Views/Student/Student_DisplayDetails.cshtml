
@{
    ViewData["Title"] = "DisplayDetails";
}

<h1>Student Display Details</h1>

<table class="table">
    <tr>
        <td>Your Roll No</td>

        <td>
            @ViewBag.Roll_No
        </td>
    </tr>
    <tr>
        <td>Your Name</td>

        <td>
            @ViewBag.Name
        </td>
    </tr>
     <tr>
        <td>Gender</td>

        <td>
            @ViewBag.Gender
        </td>
    </tr>
    <tr>
        <td>Subject</td>

        <td>
            @ViewBag.Subject
        </td>
    </tr>
    <tr>
        <td>City</td>

        <td>
            @ViewBag.City
        </td>
    </tr>
</table>



